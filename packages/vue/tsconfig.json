{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["src"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@usebool/sdk-js": ["node_modules/@usebool/sdk-js/lib/index.d.ts"]
    },
    "target": "es2020",
    "outDir": "lib",
    "module": "node16",
    "moduleResolution": "node16",
    "moduleDetection": "force",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": false,
    "lib": ["dom", "esnext"],
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true
  }
}
